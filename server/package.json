{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "frontend-install": "npm install --prefix frontend",
        "start": "node ./bin/www",
        "server": "nodemon ./bin/www",
        "frontend": "npm start --prefix frontend",
        "dev": "concurrently \"npm run server\" \"npm run frontend\"",
        "build" : "npm run build --prefix frontend",
        "postbuild" : "npm install --prefix frontend && npm run build --prefix frontend"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "concurrently": "^7.4.0",
        "connect-mongo": "^4.6.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^16.0.1",
        "express": "~4.16.1",
        "express-session": "^1.17.3",
        "googleapis": "^105.0.0",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "mongoose": "^6.5.3",
        "morgan": "~1.9.1",
        "nodemon": "^2.0.16"
    }
}